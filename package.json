{
  "name": "react-native-scaffolding",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export -p web",
    "test": "jest",
    "test:verbose": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "icons:generate": "node scripts/generate-icons.js",
    "icons:clean": "node scripts/clean-icons.js",
    "prebuild:android": "expo prebuild --platform android --non-interactive",
    "prebuild:ios": "expo prebuild --platform ios --non-interactive --clean",
    "postinstall": "pnpm run icons:generate",
    "prepare": "husky"
  },
  "dependencies": {
    "@dev-plugins/react-query": "^0.4.0",
    "@hookform/resolvers": "^5.2.2",
    "@react-navigation/native": "^7.1.8",
    "@sentry/react-native": "~7.2.0",
    "@tanstack/react-query": "^5.90.10",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "~54.0.25",
    "expo-constants": "~18.0.10",
    "expo-device": "~8.0.9",
    "expo-linking": "~8.0.9",
    "expo-localization": "~17.0.7",
    "expo-network": "~8.0.7",
    "expo-router": "~6.0.15",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.13",
    "i18next": "^25.6.3",
    "nativewind": "5.0.0-preview.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.1",
    "react-i18next": "^16.3.5",
    "react-native": "0.81.5",
    "react-native-css": "^3.0.1",
    "react-native-reanimated": "~4.1.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-web": "^0.21.2",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@eslint/js": "^9.39.1",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query-devtools": "^5.90.10",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/node": "24.10.1",
    "@types/react": "~19.1.17",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.13",
    "lightningcss": "1.30.1",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react-native-svg-transformer": "^1.5.0",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "overrides": {
    "lightningcss": "1.30.1"
  },
  "packageManager": "pnpm@10.23.0",
  "volta": {
    "node": "22.21.1",
    "pnpm": "10.23.0"
  },
  "private": true
}
